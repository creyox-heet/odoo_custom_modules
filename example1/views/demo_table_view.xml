<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="demo_table_form" model="ir.ui.view">
        <field name="name">demo.table.form</field>
        <field name="model">demo.table</field>
        <field name="arch" type="xml">
            <form string="demo table form">
               <header>
                   <h3>demo table record</h3>
                </header>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="desc"/>
                        <field name="age"/>
                        <field name="salary"/>
                        <field name="gender"/>
                        <field name="birth_date"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

        <record id="demo_table_tree" model="ir.ui.view">
        <field name="name">demo.table.tree</field>
        <field name="model">demo.table</field>
        <field name="arch" type="xml">
            <tree string="demo table tree">
                        <field name="name"/>
                        <field name="desc"/>
                        <field name="age"/>
                        <field name="salary"/>
                        <field name="gender"/>
                        <field name="birth_date"/>
            </tree>
        </field>
    </record>
    <record id="action_demo_table" model="ir.actions.act_window">
       <field name="name">Demo table</field>
       <field name="res_model">demo.table</field>
       <field name="view_mode">list,form</field>
        <field name="help">no data</field>
    </record>
    <menuitem id="menu_demo_table_root"
          name="Demo"
          sequence="1"/>

    <menuitem id="menu_demo_table_list"
          name="Demo Table1"
          parent="menu_demo_table_root"
          action="action_demo_table"
          sequence="1"/>


</odoo>