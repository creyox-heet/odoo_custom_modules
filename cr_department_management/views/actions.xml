<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="action_student_student_table" model="ir.actions.act_window">
        <field name="name">Student Table</field>
        <field name="res_model">student.student</field>
        <field name="view_mode">list,form,kanban</field>
        <field name="help">no students</field>
    </record>
    <record id="action_department_department_table" model="ir.actions.act_window">
        <field name="name">Department Table</field>
        <field name="res_model">department.department</field>
        <field name="view_mode">list,form,kanban,pivot,graph</field>
        <field name="help">no department</field>
    </record>
    <record id="action_employee_employee_table" model="ir.actions.act_window">
        <field name="name">Employee Table</field>
        <field name="res_model">employee.employee</field>
        <field name="view_mode">list,form,kanban</field>
        <field name="help">no employee</field>
    </record>
    <record id="action_department_conf_wizard" model="ir.actions.act_window">
        <field name="name">Department Configuration</field>
        <field name="res_model">department.conf</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>

</odoo>